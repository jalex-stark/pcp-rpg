# Orchestrator Configuration
# Copy this file to .env and customize

# Profile: 0=micro only, 1=+copilot, 2=+reprover
ORCH_PROFILE=1

# CPU target (0-1, e.g., 0.5 = 50%)
ORCH_CPU_TARGET=0.5

# Concurrency limits
ORCH_MAX_LIGHT=8  # Light tasks (micro, quick searches)
ORCH_MAX_HEAVY=2  # Heavy tasks (ReProver, deep Copilot)

# Difficulty threshold for escalation to heavy strategies
ORCH_ESCALATE_THRESHOLD=0.6

# LeanDojo settings (when integrated)
# LEANDOJO_CACHE_DIR=~/.cache/lean_dojo
# LEANDOJO_NUM_PROCS=4

# ReProver settings (when integrated)
# REPROVER_RETRIEVER_PATH=kaiyuy/leandojo-lean4-retriever-byt5-small
# REPROVER_GENERATOR_PATH=kaiyuy/leandojo-lean4-retriever-tacgen-byt5-small
# REPROVER_INDEX_PATH=orchestrator/cache/premise_index.faiss
# REPROVER_TOP_K=12
# REPROVER_BEAM=4

# Dashboard settings
DASHBOARD_PORT=8000
DASHBOARD_HOST=127.0.0.1
